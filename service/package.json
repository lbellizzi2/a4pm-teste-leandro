{
  "name": "service",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:quick": "pretty-quick --staged",
    "start": "node dist/src/main.js",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:generate:debug": "yarn prisma:generate && yarn start:debug",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --maxWorkers=50% --detectOpenHandles --forceExit",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:cov": "jest --coverage --maxWorkers=50%",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --detectOpenHandles --config ./test/jest-e2e.json --forceExit",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:debug": "npx prisma generate --watch",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:migrate:deploy": "npx prisma migrate deploy",
    "docker:build:local": "yarn prisma:generate && docker build -t lex-service . && docker run -d -p 3000:3000 lex-service",
    "docker:compose:dev": "docker-compose -f docker-compose.dev.yml up -d --build",
    "git:commit": "git add . && git commit -m \"update\" --no-verify && git push origin develop --no-verify"
  },
  "peerDependencies": {
    "webpack": "^5.0.0"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.15",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.0.6",
    "@nestjs/microservices": "^10.4.15",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/platform-socket.io": "^10.4.15",
    "@nestjs/schedule": "^4.1.2",
    "@nestjs/swagger": "^8.1.0",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^10.4.15",
    "@prisma/client": "^6.6.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/lodash": "^4.17.13",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "bson": "^6.10.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.5",
    "cross-var": "^1.1.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.1",
    "express-slow-down": "^2.0.3",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "nest-commander": "^3.15.0",
    "node-mailjet": "^6.0.6",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prisma": "^6.6.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.5",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.15",
    "@types/compression": "^1.7.5",
    "@types/date-fns": "^2.6.3",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.1",
    "@types/helmet": "^4.0.0",
    "@types/jest": "29.5.14",
    "@types/multer": "^1.4.12",
    "@types/node": "22.10.2",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "29.7.0",
    "pre-commit": "^1.2.2",
    "prettier": "^3.4.2",
    "pretty-quick": "^4.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": "test/.*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!<rootDir>/src/system/**/*.(t|j)s",
      "!<rootDir>/modules/**/*module.(t|j)s",
      "!<rootDir>/dist/**/*"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
